{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: Server",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev"
    },
    {
      "name": "Next.js: Client (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:65002",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "compounds": [
    {
      "name": "Next.js: Full Stack",
      "configurations": ["Next.js: Server", "Next.js: Client (Chrome)"]
    }
  ]
}
